# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Local Version
CONFIG_LOCALVERSION="-MagicTime"
CONFIG_LOCALVERSION_AUTO=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y
CONFIG_QCA_CLD_WLAN_PROFILE="qca6390"

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT
CONFIG_EXFAT_FS=y

# erofs
CONFIG_EROFS_FS=y

# Persistent store
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_CMDLINE="ramoops_memreserve=4M"
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# Backlight
CONFIG_BACKLIGHT_KTZ8866=y
CONFIG_BACKLIGHT_DUALKTZ8866=y

# Camera motor
CONFIG_MOTOR_DRV8846=y

# GPS
CONFIG_AIROHA_GPS=y

# Hall sensor
CONFIG_HALL_AKM09970=y

# ION
CONFIG_ION_CAMERA_HEAP=y
CONFIG_ION_SYSTEM_HEAP=y

# IR
CONFIG_RC_CORE=y
CONFIG_LIRC=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI=y

# Keyboard
CONFIG_INPUT_XIAOMI_KEYBOARD=y
CONFIG_KEYBOARD_NANO_8030=y

# LED
CONFIG_LEDS_AW2015=y

# Power
CONFIG_FRAME_WARN=4096
CONFIG_SMB1355_SLAVE_CHARGER=n
CONFIG_SMB1390_CHARGE_PUMP_PSY=n
CONFIG_SMB1398_CHARGER=n

# Regulator
CONFIG_I2C_RETRY=y
CONFIG_REGULATOR_PM8008=y
CONFIG_REGULATOR_QCOM_RPMH=y

# Touchscreen
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=n
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM=n
CONFIG_TOUCHSCREEN_ST=n
CONFIG_TOUCHSCREEN_COMMON=y

# USB
CONFIG_PS5169=y

# Vibrator
CONFIG_INPUT_QTI_HAPTICS=n

# Inline kernel modules
CONFIG_DVB_MPQ=y
CONFIG_DVB_MPQ_DEMUX=y
CONFIG_LCD_CLASS_DEVICE=y
CONFIG_MSM_11AD=y
CONFIG_MSM_RDBG=y
CONFIG_QCOM_LLCC_PERFMON=y
CONFIG_RMNET_PERF=y
CONFIG_RMNET_SHS=y
CONFIG_USB_GSPCA=y
CONFIG_WIGIG_SENSING_SPI=y
CONFIG_WIL6210=y

# Wireguard Support
CONFIG_WIREGUARD=y

# zRAM
CONFIG_ZRAM_WRITEBACK=y
CONFIG_ZRAM_DEDUP=y

# WALT
CONFIG_SCHED_WALT=n
CONFIG_SCHED_CORE_CTL=n
CONFIG_SCHED_TUNE=n

# PELT
CONFIG_SCHED_CASS=y
CONFIG_PELT_COMPATIBILITY_LAYER=y
CONFIG_SMP=y

# UCLAMP
CONFIG_UCLAMP_TASK=y
CONFIG_UCLAMP_ASSIST=y
CONFIG_UCLAMP_TASK_GROUP=y
CONFIG_UCLAMP_BUCKETS_COUNT=20

# CGROUP
CONFIG_CGROUPS=y
CONFIG_CGROUP_SCHED=y
CONFIG_MEMCG_SWAP_ENABLED=y
CONFIG_CGROUP_WRITEBACK=y
CONFIG_CGROUP_NET_PRIO=y

# IRQ
CONFIG_IRQ_SBALANCE=y
CONFIG_IRQ_SBALANCE_POLL_MSEC=10000
CONFIG_SBALANCE_EXCLUDE_CPUS="3,6,7"

# MIUI
CONFIG_XIAOMI_MIUI=y
CONFIG_EMERGENCY_MEMORY=y
CONFIG_CAM_RECLAIM=y
CONFIG_RTMM=y
CONFIG_MI_RECLAIM=y
CONFIG_MILLET=y
CONFIG_PERF_CRITICAL_RT_TASK=y
CONFIG_BINDER_OPT=y
CONFIG_SF_BINDER=y
CONFIG_TASK_DELAY_ACCT=y

# DEVFREQ
CONFIG_DEVFREQ_BOOST=y
CONFIG_DEVFREQ_INPUT_BOOST_DURATION_MS=58
CONFIG_DEVFREQ_CPU_LLCC_DDR_BW_BOOST_FREQ=3879

# TEO
CONFIG_CPU_IDLE_GOV_TEO=y

# PASR 
CONFIG_MEMORY_HOTPLUG=n
CONFIG_MEMORY_HOTPLUG_DEFAULT_ONLINE=n
CONFIG_MEMORY_HOTPLUG_MOVABLE_NODE=n
CONFIG_MEMORY_HOTREMOVE=n
CONFIG_QCOM_MEM_OFFLINE=n
CONFIG_OVERRIDE_MEMORY_LIMIT=n

# BFQ
CONFIG_IOSCHED_BFQ=n
CONFIG_BFQ_GROUP_IOSCHED=n

# Security
CONFIG_DEFAULT_MMAP_MIN_ADDR=32768

# SLUB
CONFIG_SLUB_CPU_PARTIAL=n

# CPUFreq
CONFIG_CPU_FREQ_TIMES=n

# CPUMASK
CONFIG_LITTLE_CPU_MASK=15
CONFIG_BIG_CPU_MASK=112
CONFIG_PRIME_CPU_MASK=128

# RD
CONFIG_RD_LZ4=y
